{
  "name": "CoolHeaded",
  "version": "0.0.1",
  "description": "add params to header",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "storage",
    "tabs",
    "*://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/icon.png"
    }
  },
  "icons": {
    "128": "images/icon.png"
  },
  "manifest_version": 2
}
